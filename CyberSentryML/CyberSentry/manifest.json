{
    "name": "Phish Detect Pro",
    "description": "Detect whether a website is malicious or safe to use",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "phish.png"
    },
    "icons": {
        "128": "phish.png"
    },
    "content_security_policy": {
        "script-src": "self",
        "object-src": "self"
    }
}